<%- include("partials/header.ejs") %>
<% if (locals.post) { %>
  <div id="content">
    <h2><%= post.title %></h2>
    <p><%= post.author %></p>
    <p><%= post.date %></p>
    <p><%= post.content %></p>
  </div>
<% } else { %>
  <h2>An error occurred while loading the post.</h2>
<% } %>
<%- include("partials/footer.ejs") %>